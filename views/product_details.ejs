  </head>

<body>
<div id="header">
<div class="container">
<div id="welcomeLine" class="row">
	<div class="span6">Welcome! <strong> <%=user.fname %> <%=user.lname %></strong></div>
	<div class="span6">
	<div class="pull-right">
		<a href="/logout"><strong> Click here to logout!</strong></a>
	</div>
	</div>
</div>
<!-- Navbar ================================================== -->
<div id="logoArea" class="navbar">
<a id="smallScreen" data-target="#topMenu" data-toggle="collapse" class="btn btn-navbar">
	<span class="icon-bar"></span>
	<span class="icon-bar"></span>
	<span class="icon-bar"></span>
</a>
  <div class="navbar-inner">
    <a class="brand" href=""><img src="/cssFilespro/images/logo.png" alt="Bootsshop"/></a>
		<form class="form-inline navbar-search" method="post" action="/productserch" >
		<input id="srchFld" class="srchTxt" type="text" />
		  <select class="srchTxt"  id="mySelect" name="mySelect">
			<option>All</option>
			<%      
                if(category.length){ 
                for(var i = 0;i < category.length;i++) { 
              %>
            <option><%=category[i].name%></option>
              <%      
                }
              } 
            %>
		</select> 
		  <button type="submit" class="btn btn-primary">Go</button>
    </form>
    <ul id="topMenu" class="nav pull-right">
	 <li class=""><a href="\home">Specials Offer</a></li>
	 <li class=""><a href="\profile">My Profile</a></li>

	 <li><a href="/viewcart" name="cart" class="btn-link"><i class="icon-shopping-cart" style="font-size:26px;color:red;"></i>Cart</a></li>
    </ul>
  </div>
</div>
</div>
</div>
<!-- Header End====================================================================== -->
<div id="mainBody">
	<div class="container">
	<div class="row">
<!-- Sidebar ================================================== -->
	<div class="col-xs-3"> 
	<div id="sidebar" class="span3">
		<div class="well well-small"><a id="myCart" href="product_summary.html"><img src="/cssFilespro/images/ico-cart.png" alt="cart">Purchase your cart <span class="badge badge-warning pull-right">$155.00</span></a></div>
		<ul id="sideManu" class="nav nav-tabs nav-stacked">
			<li class="subMenu open"><a> Category</a>
				<ul>
				<%      
                if(category.length){ 
                for(var i = 0;i < category.length;i++) { 
              	%>
				<li><a class="active" href="#"><i class="icon-chevron-right"></i><%=category[i].name %> </a></li>
				<%      
                }
            		} 
              	%>
				</ul>
			</li>
			<li class="subMenu"><a>Store </a>
			<ul style="display:none">
				<%      
                if(store.length){ 
                for(var i = 0;i < store.length;i++) { 
              	%>
				<li><a class="active" href="#"><i class="icon-chevron-right"></i><%=store[i].name %> </a></li>
				<%      
                }
            		} 
              	%>											
			</ul>
			</li>
			<li class="subMenu"><a>Generic</a>
				<ul style="display:none">
				<%      
                if(generic.length){ 
                for(var i = 0;i < generic.length;i++) { 
              	%>
				<li><a class="active" href="#"><i class="icon-chevron-right"></i><%=generic[i].name %> </a></li>
				<%      
                }
            		} 
              	%>													
			</ul>
			</li>
			<li class="subMenu"><a>Brand</a>
				<ul style="display:none">
				<%      
                if(brand.length){ 
                for(var i = 0;i < brand.length;i++) { 
              	%>
				<li><a class="active" href="#"><i class="icon-chevron-right"></i><%=brand[i].name %> </a></li>
				<%      
                }
            		} 
              	%>													
			</ul>
			</li>
			
		</ul>
		</div>
	</div>
<!-- Sidebar end=============================================== -->
	<div class="span9">
    <ul class="breadcrumb">
    <li><a >Add product to the cart for purchase..... </a></li>
    </ul>	
	<div class="row">	  
			<div id="gallery" class="span3">
            <a href="<%=inventory.imagelink1%>">
				<img src="<%=inventory.imagelink1%>" style="width:100%" />
            </a>
			<div id="differentview" class="moreOptopm carousel slide">
                <div class="carousel-inner">
                  <div class="item active">
                   <a href="<%=inventory.imagelink1%>"> <img style="width:29%" src="<%=inventory.imagelink1%>" alt=""/></a>
                   <a href="<%=inventory.imagelink2%>"> <img style="width:29%" src="<%=inventory.imagelink2%>" alt=""/></a>
                   <a href="<%=inventory.imagelink3%>" > <img style="width:29%" src="<%=inventory.imagelink3%>" alt=""/></a>
                  </div>
                </div>
			  
              </div>
			 
			</div>
			<div class="span6">
				<h3> <%=inventory.name%>  </h3>
				<small>- <%=inventory.itemcode%></small>
				<hr class="soft"/>
				<form class="form-horizontal qtyFrm" method="post" action="/addtocart">
				  <div class="control-group">
					<label class="control-label"><span>Rs. <%=inventory.retailcash%></span></label>
					<div class="controls">
					  <input type="number" min="1" class="span1" placeholder="Qty." name="crtqty"/>
					  <input type="hidden" class="span1" name="proid" value="<%=inventory.idinventory%>" />
					  <button type="submit" class="btn btn-large btn-primary pull-right"> Add to cart <i class=" icon-shopping-cart"></i></button>
					</div>
				  </div>
				</form>
				
				<hr class="soft"/>
				<h4>100 items in stock</h4>
				<hr class="soft clr"/>
				<p>

				<%=inventory.description%>

				</p>
				<a class="btn btn-small pull-right" href="">More Details</a>
				<br class="clr"/>
			<a href="#" name="detail"></a>
			<hr class="soft"/>
			</div>
			
			<div class="span9">
            <ul id="productDetail" class="nav nav-tabs">
              <li class="active"><a href="#home" data-toggle="tab">Product Details</a></li>
              <li><a href="#profile" data-toggle="tab">New Products</a></li>
            </ul>
            <div id="myTabContent" class="tab-content">
              <div class="tab-pane fade active in" id="home">
			  <h4>Product Information</h4>
                <table class="table table-bordered">
				<tbody>
				<tr class="techSpecRow"><th colspan="2">Product Details</th></tr>
				<tr class="techSpecRow"><td class="techSpecTD1">Brand: </td><td class="techSpecTD2">Fujifilm</td></tr>
				<tr class="techSpecRow"><td class="techSpecTD1">Model:</td><td class="techSpecTD2">FinePix S2950HD</td></tr>
				<tr class="techSpecRow"><td class="techSpecTD1">Released on:</td><td class="techSpecTD2"> 2011-01-28</td></tr>
				<tr class="techSpecRow"><td class="techSpecTD1">Dimensions:</td><td class="techSpecTD2"> 5.50" h x 5.50" w x 2.00" l, .75 pounds</td></tr>
				<tr class="techSpecRow"><td class="techSpecTD1">Display size:</td><td class="techSpecTD2">3</td></tr>
				</tbody>
				</table>
				
				
              </div>
		<div class="tab-pane fade" id="profile">
		
		</div>
          </div>

	</div>
</div>
</div> </div>
</div>

	<script src="/cssFilespro/themes/js/jquery.js" type="text/javascript"></script>
